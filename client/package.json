{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "concat": "^1.0.3",
    "event-stream": "^4.0.1",
    "gsap": "^2.0.2",
    "gsap-react-plugin": "^1.0.2",
    "jquery": "^1.11.1",
    "jquery-ui": "^1.12.1",
    "jquery-ui-touch-punch": "^0.2.3",
    "node-sass": "^4.9.4",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-responsive-carousel": "^3.1.43",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "react-sound": "^1.2.0",
    "react-transition-group": "^2.5.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "simplebar": "^3.1.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./public/ --include-path ./node_modules public/sass -o public/css",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./public/ --include-path ./node_modules public/sass -o public/css --watch --recursive",
    "concat:css": "concat -o public/css/style.concat.css public/css/icon-font.css public/css/style.comp.css",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "http://localhost:3001",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
